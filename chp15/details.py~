class emp:
      def em1(self):
          self.n = raw_input(">")
          self.a = raw_input(">")
          print "Name: %r" %self.n + "Age: %r" %self.a
      def em2(self):
          self.n = raw_input(">")
          self.a = raw_input(">")
          print "Name: %r" %self.n + "Age: %r" %self.a
class join:
      def joined(self):
          x = emp()
          print x.em1()
          print x.em2()

b = emp()
b.em1()
b.em2()

d = join()
d.joined()
